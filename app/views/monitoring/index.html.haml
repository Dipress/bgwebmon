%div{:id => "popup"}
	%div{:id => "popupbackground"}
	%div{:id => "popupscreen"}
		%div{:id => "popupclose"} X
		%div{:id => "popupcontent"}
%ul{:id => "content"}
	- @dialupalias.each do |u|
		%li{:class => "cli"}
			- if u.contract.online
				%div{:class => "status", :style => "background-color:#4dbe51"}
			- else
				%div{:class => "status", :style => "background-color:#e32e2e"}
			%ul
				%li
					%div="#{u.contract.title} - #{u.contract.comment}"
				%li
					%div=u.login_alias
			%table{:class => "balance"}
				%tr
					%td баланс:
					%td лимит:
				%tr
					%td 20.00
					%td="#{u.contract.closesumma}0"
			%div{:class => "clinks"}
				%a{:href => "#", :lid => u.id}Ошибки
				%a{:href => "#"}Графики
				%a{:href => "#"}Тест
				%a{:href => "#"}Платежи