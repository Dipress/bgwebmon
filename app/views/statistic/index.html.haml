%ul#nodesul.span11.offset1
	%li.nodesli
		%ul#regionul
			-@nodes.each do |n|
				- if n[:region] != "" && n[:region] != nil
					%li.noderegion
						%h3= n[:region]
						%ul.nodeul
							- n[:nodes].each do |bs|
								%li.nodetext
									= link_to bs[:title].gsub(/^([А-Я]|[а-я])+\ \-\ /,""), "/nodestatistic/#{bs[:id]}"